{
  "name": "server",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "devDependencies": {
    "@types/bun": "latest"
  },
  "scripts": {
    "dev": "bun run index.ts",
    "build": "bun build --target bun --outfile=dist/main.js index.ts",
    "start": "bun run build && bun run dist/main.js",
    "watch": "bun --watch index.ts"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@google-cloud/text-to-speech": "^6.4.0",
    "@google-cloud/vertexai": "^1.10.0",
    "axios": "^1.13.2",
    "google-auth-library": "^10.5.0",
    "ioredis": "^5.8.2",
    "livekit-server-sdk": "^2.15.0"
  }
}
